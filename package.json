{
  "name": "jsgrest",
  "version": "0.0.6",
  "description": "Postgres REST API server in JavaScript (a la PostgREST)",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "bin": {
    "jsgrest": "dist/cli/index.js"
  },
  "scripts": {
    "build": "eslint src demo test && npm run build-peg && cross-env BABEL_ENV=production babel -D src -d dist && dependency-check package.json --no-default-entries --entry dist/cli/index.js",
    "build-peg": "node build-peg src/peg/**.pegjs",
    "test": "cross-env USE_PEGJS_REQUIRE=1 mocha && npm run test-dist",
    "test-dist": "node build-test && mocha --opts mocha-dist.opts test-dist",
    "prepublish": "npm run build",
    "eslint": "eslint src/**/*.js  demo/**/*.js test/**/*.js",
    "eclint": "eclint check *.js *.json *.sublime-project *.md *.yml *.*rc *.*ignore src/**/*.js demo/**/*.js test/**/*.js",
    "ecfix": "eclint fix *.js *.json *.sublime-project *.md *.yml *.*rc *.*ignore src/**/*.js  demo/**/*.js test/**/*.js",
    "check-style": "npm run eclint && npm run eslint",
    "dev": "cross-env USE_PEGJS_REQUIRE=1 chokidar -d 1000 -t 10000 \"src/\" \"test/\" package.json  .babelrc  --initial -i \"*test/data*\" -c \"mocha\"",
    "dev-unit": "cross-env USE_PEGJS_REQUIRE=1 chokidar -d 1000 -t 10000 \"src/\" \"test/\" package.json  .babelrc  --initial -i \"*test/data*\" -c \"mocha test/unit/**/*.js\"",
    "dev-integration": "cross-env USE_PEGJS_REQUIRE=1 chokidar -d 1000 -t 10000 \"src/\" \"test/\" package.json  .babelrc  --initial -i \"*test/data*\" -c \"mocha test/integration/**/*.js\"",
    "nodemon": "cross-env USE_PEGJS_REQUIRE=1 nodemon --exec babel-node -- src/cli",
    "coveralls": "cross-env USE_PEGJS_REQUIRE=1 istanbul cover -x **/*.pegjs.js ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "author": "Moti Zilberman <motiz88@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motiz88/jsgrest.git"
  },
  "bugs": {
    "url": "https://github.com/motiz88/jsgrest/issues",
    "email": "motiz88@gmail.com"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.4.0",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015-node4": "^2.0.3",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "chai-http": "^2.0.1",
    "chai-subset": "^1.2.0",
    "chai-things": "^0.2.0",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.6",
    "cross-env": "^1.0.7",
    "dependency-check": "^2.5.1",
    "documentation": "^3.0.4",
    "eclint": "^1.1.5",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^1.10.3",
    "eslint-plugin-flow-vars": "^0.1.3",
    "fs-extra-promise": "^0.3.1",
    "glob": "^7.0.0",
    "glob-promise": "^1.0.6",
    "istanbul": "^1.0.0-alpha.2",
    "jscodeshift": "^0.3.13",
    "lodash": "^4.16.1",
    "longjohn": "^0.2.11",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "mock-express-request": "^0.1.1",
    "mock-express-response": "^0.1.1",
    "mz": "^2.3.0",
    "node-fetch": "^1.3.3",
    "nodemon": "^1.8.1",
    "pegjs": "^0.9.0",
    "shell-escape-tag": "^1.0.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "@motiz88/pg": "^4.5.0",
    "argparse": "^1.0.4",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.2",
    "content-range": "^1.0.0",
    "express": "^4.13.4",
    "pg-escape": "motiz88/pg-escape",
    "xregexp": "^3.0.0"
  },
  "engines": {
    "node": ">=4"
  }
}
